function zdot = ex_5_6_damp(t, z, phi)
% Consider the following simple linear ODE of second order
% xddot + x = F0 * cos(w * t) 
% 1. Convert the equation to set of first-order ODEs
% z1 = x, z2 = xdot 
if nargin < 3 
    phi = 0.7
end
F0 = 1;
w = 1; 
zdot = zeros()
for i = 1:length(phi)
    zdot(:, :, i) = [z(2); F0 * cos(w * t) - phi(i) * z(2) - z(1)];
end

